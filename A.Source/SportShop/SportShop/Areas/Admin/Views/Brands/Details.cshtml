@model SportShop.Entities.Brand

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header" style="margin-top:20px">Quản lý nhãn hàng</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div>
    <dl class="dl-horizontal">
        <dt>
            <span>Tên nhãn hàng</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.BrandName)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Sửa", "Edit", new { id = Model.BrandID }, htmlAttributes: new { @class = "btn btn-default" })
    @Html.ActionLink("Quay lại", "Index", null, htmlAttributes: new { @class = "btn btn-default" })
</p>
