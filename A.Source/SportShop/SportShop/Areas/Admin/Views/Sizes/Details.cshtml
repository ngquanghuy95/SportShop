@model SportShop.Entities.Size

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header" style="margin-top:20px">Quản lý size</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div>

    <dl class="dl-horizontal">
        <dt>
            <span>Số size</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SizeNum)
        </dd>

        <dt>
            <span>Mô tả</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Sửa", "Edit", new { id = Model.SizeID },htmlAttributes: new { @class = "btn btn-default" }) 
    @Html.ActionLink("Quay lại", "Index", null, htmlAttributes: new { @class = "btn btn-default" })
</p>
